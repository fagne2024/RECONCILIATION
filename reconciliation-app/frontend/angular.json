{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "csv-reconciliation": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/csv-reconciliation",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                          "src/styles.scss"
                        ],
                        "scripts": [],
                        "allowedCommonJsDependencies": [
                            "papaparse",
                            "exceljs",
                            "file-saver"
                        ],
                        "webWorkerTsConfig": "tsconfig.worker.json"
                    },
                    "configurations": {
                        "production": {
                                                    "budgets": [
                            {
                                "type": "initial",
                                "maximumWarning": "10mb",
                                "maximumError": "20mb"
                            },
                            {
                                "type": "anyComponentStyle",
                                "maximumWarning": "50kb",
                                "maximumError": "100kb"
                            }
                        ],
                        "sourceMap": false,
                        "optimization": false,
                        "buildOptimizer": false,
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": false,
                            "namedChunks": true,
                            "aot": false
                        },
                        "worker": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": false,
                            "namedChunks": true,
                            "aot": false,
                            "tsConfig": "tsconfig.worker.json"
                        },
                        "decorators": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": false,
                            "namedChunks": true,
                            "aot": false,
                            "tsConfig": "tsconfig.decorators.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "csv-reconciliation:build:production"
                        },
                        "development": {
                            "browserTarget": "csv-reconciliation:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "csv-reconciliation:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                          "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    }
} 